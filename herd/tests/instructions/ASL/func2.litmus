ASL func02
(* Some basic getters/setters *)

{}

getter X(i:integer) => integer
begin
    return i;
end;

setter X(i:integer) = v:integer
begin
    let internal_i = i;
    let internal_v = v;
end;

func main() => integer
begin
    X(2) = 3;
    let x = X(4);

    return 0;
end;

forall (
     0: main.0.x = 4
  /\ 0: X-1.0.internal_i = 2
  /\ 0: X-1.0.internal_v = 3
)

